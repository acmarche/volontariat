{
  "name": "acmarche/volontariat",
  "description": "Platforme de rencontre entre les volontaires et les Associations",
  "homepage": "https://github.com/acmarche/volontariat",
  "license": "proprietary",
  "type": "project",
  "authors": [
    {
      "name": "Jean-François Sénéchal",
      "email": "informatique@marche.be",
      "homepage": "http://cst.marche.be",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "friendsofsymfony/ckeditor-bundle": "^2.2",
    "h4cc/wkhtmltopdf-amd64": "^0.12.4",
    "knplabs/knp-snappy-bundle": "^1.7",
    "liip/imagine-bundle": "^2.3",
    "phpoffice/phpspreadsheet": "^1.6",
    "symfony/swiftmailer-bundle": "^3.1",
    "vich/uploader-bundle": "^1.13",
    "beberlei/doctrineextensions": "^1.2",
    "knplabs/doctrine-behaviors": "^2.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App",
      "AcMarche\\Volontariat\\": "src/AcMarche/Volontariat/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "ckeditor:install --clear=drop": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "5.1.*"
    }
  }
}

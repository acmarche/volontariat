{{ form_start(form_delete) }}
{{ form_errors(form_delete) }}
{{ form_row(form_delete.submit) }}

{% for image in images %}
    <div class="pull-left center-block" style="margin: 10px; outline: 1px #28ff3d solid;">
        <p class="text-center">
            <label>
                <input type="checkbox" name="img[]" value="{{ image.name }}"/>
            </label>
        </p>
        <a href="{{ image.url }}" title="{{ image.name }}" data-gallery>
            <img src="{{ image.url | imagine_filter('acmarche_volontariat_edit_thumb') }}" alt="img" class="img-thumbnail"/>
        </a>
    </div>
{% endfor %}
{{ form_end(form_delete) }}
<div class="hidden-print">
    {{ form_start(form, { 'attr': {'class': 'row row-cols-lg-auto g-3 align-items-center hidden-print'}}) }}
    {{ form_errors(form) }}
    {{ form_row(form.nom, {label_attr: {class: 'sr-only'}, attr:{'class': 'my-1 mr-sm-2'} }) }}
    <br/><br/>

    <a href="{{ path('volontariat_admin_message_new', { 'query': 'association' }) }}" role="button"
       class="btn btn-primary"><i
                class="ti ti-envelope"></i> Envoyer un message</a>

    {{ form_end(form) }}
    <br/><br/>
</div>
